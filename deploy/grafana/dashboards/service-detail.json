{
  "__inputs": [],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "9.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": []
  },
  "description": "Детальный статус зависимостей выбранного сервиса",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "arrow-left",
      "includeVars": false,
      "keepTime": true,
      "tags": [],
      "targetBlank": false,
      "title": "Назад к обзору",
      "type": "link",
      "url": "/d/dephealth-overview/dephealth-overview"
    }
  ],
  "panels": [
    {
      "title": "Текущее состояние",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "title": "Статус зависимостей",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 24, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "color": { "mode": "thresholds" },
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } } }
          ]
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none"
      },
      "targets": [
        {
          "expr": "app_dependency_health{job=~\"$service\"}",
          "legendFormat": "{{dependency}} ({{host}}:{{port}})",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Таблица endpoint-ов",
      "type": "table",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 5 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "filterable": true
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Статус" },
            "properties": [
              { "id": "custom.displayMode", "value": "color-background-solid" },
              {
                "id": "mappings",
                "value": [
                  { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
                ]
              },
              { "id": "custom.width", "value": 100 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Латентность (p99)" },
            "properties": [
              { "id": "unit", "value": "s" },
              { "id": "decimals", "value": 3 },
              { "id": "custom.displayMode", "value": "color-text" },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "green", "value": null },
                    { "color": "yellow", "value": 0.1 },
                    { "color": "red", "value": 1 }
                  ]
                }
              }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Средняя латентность" },
            "properties": [
              { "id": "unit", "value": "s" },
              { "id": "decimals", "value": 3 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Time" },
            "properties": [
              { "id": "custom.hidden", "value": true }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Статус", "desc": false }],
        "footer": { "show": false }
      },
      "transformations": [
        { "id": "merge", "options": {} },
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "dependency": "Зависимость",
              "type": "Тип",
              "host": "Host",
              "port": "Port",
              "Value #A": "Статус",
              "Value #B": "Латентность (p99)",
              "Value #C": "Средняя латентность"
            },
            "indexByName": {
              "dependency": 0,
              "type": 1,
              "host": 2,
              "port": 3,
              "Value #A": 4,
              "Value #B": 5,
              "Value #C": 6
            }
          }
        }
      ],
      "targets": [
        {
          "expr": "app_dependency_health{job=~\"$service\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, rate(app_dependency_latency_seconds_bucket{job=~\"$service\"}[5m]))",
          "format": "table",
          "instant": true,
          "refId": "B"
        },
        {
          "expr": "rate(app_dependency_latency_seconds_sum{job=~\"$service\"}[5m]) / rate(app_dependency_latency_seconds_count{job=~\"$service\"}[5m])",
          "format": "table",
          "instant": true,
          "refId": "C"
        }
      ]
    },
    {
      "title": "Здоровье зависимостей (24ч)",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false
    },
    {
      "title": "app_dependency_health",
      "type": "state-timeline",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 14 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "lineWidth": 0,
            "fillOpacity": 80
          },
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
          ]
        },
        "overrides": []
      },
      "options": {
        "showValue": "auto",
        "mergeValues": true,
        "rowHeight": 0.8,
        "alignValue": "center",
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "app_dependency_health{job=~\"$service\"}",
          "legendFormat": "{{dependency}} ({{host}}:{{port}})",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Латентность",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "collapsed": false
    },
    {
      "title": "Латентность проверок (heatmap)",
      "type": "heatmap",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 23 },
      "fieldConfig": {
        "defaults": {
          "custom": {}
        },
        "overrides": []
      },
      "options": {
        "calculate": false,
        "cellGap": 1,
        "color": {
          "exponent": 0.5,
          "fill": "dark-orange",
          "mode": "scheme",
          "scale": "exponential",
          "scheme": "Oranges",
          "steps": 64
        },
        "yAxis": {
          "unit": "s"
        },
        "tooltip": {
          "show": true,
          "yHistogram": true
        },
        "exemplars": { "color": "rgba(255,0,255,0.7)" },
        "filterValues": { "le": 1e-9 },
        "legend": { "show": true },
        "rowsFrame": { "layout": "auto" },
        "showValue": "never"
      },
      "targets": [
        {
          "expr": "sum by (le) (rate(app_dependency_latency_seconds_bucket{job=~\"$service\"}[$__rate_interval]))",
          "legendFormat": "{{le}}",
          "format": "heatmap",
          "refId": "A"
        }
      ]
    },
    {
      "title": "P99 латентность по зависимости",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 31 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "lastNotNull"] }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(app_dependency_latency_seconds_bucket{job=~\"$service\"}[5m]))",
          "legendFormat": "{{dependency}} ({{host}}:{{port}})",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Средняя латентность по зависимости",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 31 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "lastNotNull"] }
      },
      "targets": [
        {
          "expr": "rate(app_dependency_latency_seconds_sum{job=~\"$service\"}[5m]) / rate(app_dependency_latency_seconds_count{job=~\"$service\"}[5m])",
          "legendFormat": "{{dependency}} ({{host}}:{{port}})",
          "refId": "A"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": ["dephealth", "dependencies", "service-detail"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "label": "Datasource"
      },
      {
        "name": "service",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(app_dependency_health, job)",
        "refresh": 2,
        "includeAll": false,
        "multi": false,
        "current": {},
        "hide": 0,
        "label": "Сервис"
      }
    ]
  },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "dephealth: Детали сервиса",
  "uid": "dephealth-service-detail",
  "version": 1
}
