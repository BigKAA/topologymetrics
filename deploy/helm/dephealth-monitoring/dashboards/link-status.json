{
  "__inputs": [],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" }
  ],
  "annotations": {
    "list": [
      {
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "enable": true,
        "expr": "changes(app_dependency_health{dependency=~\"$dependency\", host=~\"$host\", port=~\"$port\"}[1m]) > 0",
        "hide": false,
        "iconColor": "rgba(255, 96, 96, 1)",
        "name": "Изменения состояния",
        "tagKeys": "job",
        "titleFormat": "{{job}}: {{dependency}}"
      }
    ]
  },
  "description": "Подробная информация о конкретном соединении",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "arrow-left",
      "includeVars": false,
      "keepTime": true,
      "tags": [],
      "targetBlank": false,
      "title": "Service List",
      "type": "link",
      "url": "/d/dephealth-service-list/"
    },
    {
      "asDropdown": false,
      "icon": "link",
      "includeVars": false,
      "keepTime": true,
      "tags": [],
      "targetBlank": false,
      "title": "Links Status",
      "type": "link",
      "url": "/d/dephealth-links-status/"
    }
  ],
  "panels": [
    {
      "id": 1,
      "title": "Текущий статус",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 5, "w": 8, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "color": { "mode": "thresholds" },
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN" }, "1": { "text": "UP" } } }
          ]
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "min by (dependency, host, port, job) (app_dependency_health{dependency=~\"$dependency\", host=~\"$host\", port=~\"$port\"})",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "title": "SLA / Доступность",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 5, "w": 8, "x": 8, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 95 },
              { "color": "green", "value": 99 }
            ]
          },
          "color": { "mode": "thresholds" },
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "avg_over_time(min by (dependency, host, port, job) (app_dependency_health{dependency=~\"$dependency\", host=~\"$host\", port=~\"$port\"})[$__range]) * 100",
          "legendFormat": "{{job}}",
          "refId": "A",
          "instant": true,
          "range": false
        }
      ]
    },
    {
      "id": 3,
      "title": "Flapping (15 мин)",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 5, "w": 8, "x": 16, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 4 },
              { "color": "red", "value": 8 }
            ]
          },
          "color": { "mode": "thresholds" },
          "noValue": "0"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "max by (dependency, host, port, job) (changes(app_dependency_health{dependency=~\"$dependency\", host=~\"$host\", port=~\"$port\"}[15m]))",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 10,
      "title": "Timeline",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false
    },
    {
      "id": 4,
      "title": "Здоровье соединения (timeline)",
      "type": "state-timeline",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 6 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "lineWidth": 0,
            "fillOpacity": 80
          },
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "mappings": [
            { "type": "value", "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } } }
          ]
        },
        "overrides": []
      },
      "options": {
        "showValue": "auto",
        "mergeValues": true,
        "rowHeight": 0.8,
        "alignValue": "center",
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "app_dependency_health{dependency=~\"$dependency\", host=~\"$host\", port=~\"$port\"}",
          "legendFormat": "{{job}}: {{dependency}} ({{host}}:{{port}})",
          "refId": "A"
        }
      ]
    },
    {
      "id": 11,
      "title": "Латентность",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 12 },
      "collapsed": false
    },
    {
      "id": 5,
      "title": "Латентность (heatmap)",
      "type": "heatmap",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 13 },
      "fieldConfig": {
        "defaults": { "custom": {} },
        "overrides": []
      },
      "options": {
        "calculate": false,
        "cellGap": 1,
        "color": {
          "exponent": 0.5,
          "fill": "dark-orange",
          "mode": "scheme",
          "scale": "exponential",
          "scheme": "Oranges",
          "steps": 64
        },
        "yAxis": { "unit": "s" },
        "tooltip": { "show": true, "yHistogram": true },
        "exemplars": { "color": "rgba(255,0,255,0.7)" },
        "filterValues": { "le": 1e-9 },
        "legend": { "show": true },
        "rowsFrame": { "layout": "auto" },
        "showValue": "never"
      },
      "targets": [
        {
          "expr": "sum by (le) (rate(app_dependency_latency_seconds_bucket{dependency=~\"$dependency\", host=~\"$host\", port=~\"$port\"}[$__rate_interval]))",
          "legendFormat": "{{le}}",
          "format": "heatmap",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "title": "P99 латентность",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 13 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto",
            "thresholdsStyle": { "mode": "dashed" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "lastNotNull"] }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (job, le) (rate(app_dependency_latency_seconds_bucket{dependency=~\"$dependency\", host=~\"$host\", port=~\"$port\"}[5m])))",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 7,
      "title": "Средняя латентность",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 21 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "lastNotNull"] }
      },
      "targets": [
        {
          "expr": "sum by (job) (rate(app_dependency_latency_seconds_sum{dependency=~\"$dependency\", host=~\"$host\", port=~\"$port\"}[5m])) / sum by (job) (rate(app_dependency_latency_seconds_count{dependency=~\"$dependency\", host=~\"$host\", port=~\"$port\"}[5m]))",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["dephealth"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "label": "Datasource"
      },
      {
        "name": "dependency",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(app_dependency_health, dependency)",
        "refresh": 2,
        "includeAll": false,
        "multi": false,
        "current": {},
        "hide": 0,
        "label": "Зависимость"
      },
      {
        "name": "host",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(app_dependency_health{dependency=~\"$dependency\"}, host)",
        "refresh": 2,
        "includeAll": false,
        "multi": false,
        "current": {},
        "hide": 0,
        "label": "Хост"
      },
      {
        "name": "port",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(app_dependency_health{dependency=~\"$dependency\", host=~\"$host\"}, port)",
        "refresh": 2,
        "includeAll": false,
        "multi": false,
        "current": {},
        "hide": 0,
        "label": "Порт"
      }
    ]
  },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "dephealth: Link Status",
  "uid": "dephealth-link-status",
  "version": 1
}
