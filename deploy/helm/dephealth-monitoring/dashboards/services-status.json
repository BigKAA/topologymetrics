{
  "__inputs": [],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" }
  ],
  "annotations": {
    "list": [
      {
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "enable": false,
        "expr": "changes(app_dependency_health{namespace=~\"$namespace\"}[1m]) > 0",
        "hide": false,
        "iconColor": "rgba(255, 96, 96, 1)",
        "name": "Изменения состояния",
        "tagKeys": "dependency,host",
        "titleFormat": "{{dependency}} @ {{host}}:{{port}}"
      }
    ]
  },
  "description": "Состояние системы на промежутке времени — timeline, графики, heatmap",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [
    {
      "asDropdown": false,
      "icon": "apps",
      "includeVars": true,
      "keepTime": true,
      "tags": [],
      "targetBlank": false,
      "title": "Service List",
      "type": "link",
      "url": "/d/dephealth-service-list/"
    },
    {
      "asDropdown": false,
      "icon": "link",
      "includeVars": true,
      "keepTime": true,
      "tags": [],
      "targetBlank": false,
      "title": "Links Status",
      "type": "link",
      "url": "/d/dephealth-links-status/"
    }
  ],
  "panels": [
    {
      "id": 1,
      "title": "OK",
      "description": "Все зависимости доступны",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "green", "mode": "fixed" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "noValue": "0"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "count(min by (job, namespace) (app_dependency_health{namespace=~\"$namespace\", type=~\"$dependency_type\"}) == 1) or vector(0)",
          "legendFormat": "",
          "refId": "A",
          "instant": true,
          "range": false
        }
      ]
    },
    {
      "id": 2,
      "title": "Degraded",
      "description": "Часть зависимостей недоступна",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "orange", "mode": "fixed" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "orange", "value": null }] },
          "noValue": "0"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "count((min by (job, namespace) (app_dependency_health{namespace=~\"$namespace\", type=~\"$dependency_type\"}) == 0) and (max by (job, namespace) (app_dependency_health{namespace=~\"$namespace\", type=~\"$dependency_type\"}) == 1)) or vector(0)",
          "legendFormat": "",
          "refId": "A",
          "instant": true,
          "range": false
        }
      ]
    },
    {
      "id": 3,
      "title": "Broken",
      "description": "Все зависимости недоступны",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "red", "mode": "fixed" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }] },
          "noValue": "0"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center"
      },
      "targets": [
        {
          "expr": "count(max by (job, namespace) (app_dependency_health{namespace=~\"$namespace\", type=~\"$dependency_type\"}) == 0) or vector(0)",
          "legendFormat": "",
          "refId": "A",
          "instant": true,
          "range": false
        }
      ]
    },
    {
      "id": 10,
      "title": "Состояние сервисов (timeline)",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 4 },
      "collapsed": false
    },
    {
      "id": 4,
      "title": "Состояние сервисов (только проблемные)",
      "description": "OK=2 (зелёный), Degraded=1 (оранжевый), Broken=0 (красный). Показаны только сервисы, имевшие проблемы за выбранный период.",
      "type": "state-timeline",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 5 },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "lineWidth": 0,
            "fillOpacity": 80
          },
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "green", "value": 2 }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "Broken", "color": "red" },
                "1": { "text": "Degraded", "color": "orange" },
                "2": { "text": "OK", "color": "green" }
              }
            }
          ],
          "links": [
            {
              "title": "Статус сервиса \u2192",
              "url": "/d/dephealth-service-status/?var-service=${__series.name}&${__url_time_range}",
              "targetBlank": false
            }
          ]
        },
        "overrides": []
      },
      "options": {
        "showValue": "auto",
        "mergeValues": true,
        "rowHeight": 0.8,
        "alignValue": "center",
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "(min by (job, namespace) (app_dependency_health{namespace=~\"$namespace\", type=~\"$dependency_type\"}) + max by (job, namespace) (app_dependency_health{namespace=~\"$namespace\", type=~\"$dependency_type\"})) and on(job, namespace) (min_over_time((min by (job, namespace) (app_dependency_health{namespace=~\"$namespace\", type=~\"$dependency_type\"}))[${__range}:15s]) < 1)",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 11,
      "title": "Метрики",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false
    },
    {
      "id": 5,
      "title": "Здоровье зависимостей",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "stepAfter",
            "lineWidth": 2,
            "fillOpacity": 20,
            "pointSize": 5,
            "showPoints": "never"
          },
          "min": 0,
          "max": 1,
          "links": [
            {
              "title": "Статус соединения \u2192",
              "url": "/d/dephealth-link-status/?var-dependency=${__field.labels.dependency}&var-host=${__field.labels.host}&var-port=${__field.labels.port}&${__url_time_range}",
              "targetBlank": false
            }
          ]
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "min by (dependency, host, port, job) (app_dependency_health{namespace=~\"$namespace\", type=~\"$dependency_type\"})",
          "legendFormat": "{{job}}: {{dependency}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "title": "Латентность (heatmap)",
      "type": "heatmap",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "fieldConfig": {
        "defaults": { "custom": {} },
        "overrides": []
      },
      "options": {
        "calculate": false,
        "cellGap": 1,
        "color": {
          "exponent": 0.5,
          "fill": "dark-orange",
          "mode": "scheme",
          "scale": "exponential",
          "scheme": "Oranges",
          "steps": 64
        },
        "yAxis": { "unit": "s" },
        "tooltip": { "show": true, "yHistogram": true },
        "exemplars": { "color": "rgba(255,0,255,0.7)" },
        "filterValues": { "le": 1e-9 },
        "legend": { "show": true },
        "rowsFrame": { "layout": "auto" },
        "showValue": "never"
      },
      "targets": [
        {
          "expr": "sum by (le) (rate(app_dependency_latency_seconds_bucket{namespace=~\"$namespace\", type=~\"$dependency_type\"}[$__rate_interval]))",
          "legendFormat": "{{le}}",
          "format": "heatmap",
          "refId": "A"
        }
      ]
    },
    {
      "id": 12,
      "title": "Латентность",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "collapsed": false
    },
    {
      "id": 7,
      "title": "P99 латентность по сервисам",
      "description": "Пороги: >1с — warning, >5с — critical",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 23 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto",
            "thresholdsStyle": { "mode": "dashed" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "links": [
            {
              "title": "Статус сервиса \u2192",
              "url": "/d/dephealth-service-status/?var-service=${__field.labels.job}&${__url_time_range}",
              "targetBlank": false
            }
          ]
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "lastNotNull"] }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (job, le) (rate(app_dependency_latency_seconds_bucket{namespace=~\"$namespace\", type=~\"$dependency_type\"}[5m])))",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["dephealth"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "label": "Datasource"
      },
      {
        "name": "namespace",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(app_dependency_health, namespace)",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "hide": 0,
        "label": "Namespace"
      },
      {
        "name": "dependency_type",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(app_dependency_health{namespace=~\"$namespace\"}, type)",
        "refresh": 2,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "hide": 0,
        "label": "Тип"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "dephealth: Services Status",
  "uid": "dephealth-services-status",
  "version": 1
}
